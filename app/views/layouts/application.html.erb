<!DOCTYPE html>
<html>
<head>
  <%= favicon_link_tag %>
  <title>Nexproc</title>

  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body style="background-color: #555555">
  <%= flash[:errors] if flash[:errors] %>
  <%if current_user %>
    <%= link_to "Logout", sessions_url, method: :delete %>
  <% end %>


<%= yield %>

</body>
</html>
