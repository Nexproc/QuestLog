<h2>Sign In!</h2>
<form action="<%=sessions_url%>" method="post">
  <%= auth_token %>
  <p><input type="text" name="user[username]" value="<%=@user.username%>" placeholder="Username"></p>
  <p><input type="password" name="user[password]" placeholder="Password"></p>
  <input type="submit" value="Log In!">
</form>

<%= link_to 'Sign Up', new_user_url %>
